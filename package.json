{
  "name": "icon-source",
  "version": "1.1.0",
  "packageManager": "yarn@4.12.0",
  "private": true,
  "workspaces": [
    "buildSrc",
    "doki-build-source"
  ],
  "scripts": {
    "getDokiBuildSrc": "git clone https://github.com/ZimCodes/doki-build-source.git",
    "buildDokiDep": "yarn workspace doki-build-source build",
    "buildIconsDep": "yarn workspace icon-build-source build",
    "cleanIcons": "yarn workspace icon-build-source cleanIcons",
    "buildDeps": "yarn buildDokiDep && yarn buildIconsDep",
    "buildIcons": "yarn buildDeps && yarn workspace icon-build-source start"
  }
}
